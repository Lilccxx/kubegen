            periodSeconds: 3
- apiVersion: extensions/v1beta1
  kind: Deployment
  metadata:
    labels:
      name: catalogue-db
    name: catalogue-db
  spec:
    replicas: 1
    selector:
      matchLabels:
        name: catalogue-db
    template:
      metadata:
        labels:
          name: catalogue-db
      spec:
        containers:
        - env:
          - name: MYSQL_DATABASE
            value: socksdb
          - name: MYSQL_ROOT_PASSWORD
            value: fake_password
