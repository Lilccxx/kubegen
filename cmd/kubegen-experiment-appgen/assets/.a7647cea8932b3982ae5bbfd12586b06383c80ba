---
apiVersion: v1
items:
- apiVersion: extensions/v1beta1
  kind: Deployment
  metadata:
    labels:
      name: nginx
    name: nginx
  spec:
    replicas: 1
    selector:
      matchLabels:
        name: nginx
    template:
      metadata:
        creationTimestamp: null
        labels:
          name: nginx
      spec:
        containers:
        - image: nginx:latest
          name: nginx
          ports:
          - containerPort: 80
            name: nginx
          resources: {}
- apiVersion: v1
  kind: Service
  metadata:
    labels:
      name: nginx
    name: nginx
  spec:
    ports:
    - name: nginx
      port: 80
      targetPort: nginx
    selector:
      name: nginx
kind: List

